{
  "extends": [
    "next/core-web-vitals", 
    "eslint:recommended"
  ],
  "rules": {
    "no-console": ["warn"],
    "no-unused-vars": "warn",
    "indent": ["error", 2],
    "keyword-spacing": ["error", { "before": true }],
    "linebreak-style": "off",
    "object-curly-spacing": ["error", "always"],
    "arrow-spacing": "warn"
  }
}

